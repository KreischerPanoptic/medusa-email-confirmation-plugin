      "@medusajs/ui": "^3.0.1",
      "@types/node": "^20.14.12",
      "awilix": "^8.0.1",
      "cors": "^2.8.5",
      "cross-env": "^7.0.3",
      "express": "^4.17.2",
      "shx": "^0.3.4",
      "typescript": "^5.2.2"
   },
